{
  "name": "tw2gem-ai-call-center",
  "version": "1.0.0",
  "description": "TW2GEM AI Call Center - Complete monorepo with Twilio-Gemini integration and management UI",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "node start-ai-call-center.js",
    "deploy": "node start-ai-call-center.js",
    "dev": "node server.js",
    "build": "npm run build --workspaces",
    "lint": "npm run lint --workspaces",
    "lint:fix": "npm run lint:fix --workspaces",
    "format": "prettier --write .",
    "clean": "npm run clean --workspaces && rm -rf dist",
    "start-ai-call-center": "node start-ai-call-center.js",
    "production-deploy": "node start-ai-call-center.js",
    "health": "curl http://localhost:12001/health",
    "ui:dev": "npm run dev --workspace=packages/ui",
    "ui:build": "npm run build --workspace=packages/ui",
    "server:dev": "npm run dev --workspace=packages/server"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@google/genai": "^1.7.0",
    "@supabase/supabase-js": "^2.50.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.6.1",
    "express": "^4.21.1",
    "node-fetch": "^3.3.2",
    "stripe": "^18.2.1",
    "twilio": "^5.7.1",
    "uuid": "^11.1.0",
    "wavefile": "^11.0.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.20.7",
    "@types/node": "^24.0.3",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.0.0",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "tw2gem",
    "twilio",
    "gemini",
    "ai",
    "voice",
    "call-center",
    "monorepo",
    "websocket",
    "real-time"
  ],
  "author": "AI Call Center Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/movefreely22/ai-new-V1.git"
  }
}
